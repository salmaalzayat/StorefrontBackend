{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "semi": "error",
    "@typescript-eslint/no-var-requires": "off"
  }
}
